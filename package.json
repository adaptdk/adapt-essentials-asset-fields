{
  "name": "asset-fields",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "upload": "contentful-app-scripts upload --bundle-dir ./dist",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./dist  --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN"
  },
  "dependencies": {
    "@contentful/app-sdk": "^4.19.1",
    "@contentful/f36-avatar": "^4.0.0-alpha.8",
    "@contentful/f36-components": "^4.43.0",
    "@contentful/f36-icons": "^4.25.0",
    "@contentful/f36-image": "^4.0.0-alpha.0",
    "@contentful/f36-workbench": "^4.21.0",
    "@contentful/react-apps-toolkit": "^1.2.15",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@uidotdev/usehooks": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-focus-lock": "^2.9.4",
    "react-router-dom": "^6.13.0",
    "use-local-storage-extended": "^0.1.1"
  },
  "devDependencies": {
    "@contentful/app-scripts": "^1.10.2",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "happy-dom": "^9.20.3",
    "type-fest": "^4.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  }
